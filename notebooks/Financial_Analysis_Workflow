   "source": [
    "# Ha a Payment_Date üres, akkor a mai naphoz képest nézzük (vagy 0-nak vesszük)\n",
    "df_clean['Days_Overdue'] = (df_clean['Payment_Date'] - df_clean['Due_Date']).dt.days\n",
    "df_clean['Days_Overdue'] = df_clean['Days_Overdue'].fillna(0).astype(int)"
   ]
